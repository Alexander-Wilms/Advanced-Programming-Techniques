#include <iostream>
#include "CWaypoint.h"

using namespace std;

int main() {
	CWaypoint amsterdam = CWaypoint("Amsterdam", 52.370197222222, 4.8904444444444);
	CWaypoint darmstadt = CWaypoint("Darmstadt", 49.872833, 8.651222);
	CWaypoint berlin = CWaypoint("Berlin", 52.518611, 13.408333);
	CWaypoint tokio = CWaypoint("Tokio", 35.683889, 139.774444);

	cout << "Sie haben Ihr Ziel erreicht" << endl;
	return 0;
}
